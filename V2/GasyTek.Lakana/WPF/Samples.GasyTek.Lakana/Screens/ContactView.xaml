<UserControl
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	mc:Ignorable="d"
	x:Class="Samples.GasyTek.Lakana.Screens.ContactView"
	d:DesignWidth="640" 
	d:DesignHeight="480"
	Style="{StaticResource ScreenStyle}" 
	Background="{DynamicResource WorkspaceBackgroundBrush}">

	<DockPanel x:Name="LayoutRoot">
		<TextBlock DockPanel.Dock="Top" 
					Text="Contact"
					Foreground="{DynamicResource WindowBackgorundBrush}" Style="{DynamicResource ScreenTitleStyle}" />
			
	</DockPanel>
</UserControl>