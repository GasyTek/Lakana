             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:lakana="http://schemas.gasytek.com/wpf/lakana" mc:Ignorable="d" 
             x:Class="Samples.GasyTek.Lakana.App"
             StartupUri="Shell.xaml">
